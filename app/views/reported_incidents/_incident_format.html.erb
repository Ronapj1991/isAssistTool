<% @reported_incidents.each do |incident| %>
  <table>
    <tr>
      <td><strong>Incident ID</strong></td>
      <td><%= incident.incident_id %></td>
    </tr>
    <tr>
      <td>Resolution</td>
      <td><%= incident.resolution %></td>
    </tr>
    <tr>
      <td>Resolved By</td>
      <td><%= incident.resolved_by %></td>
    </tr>
    <tr>  
      <td>Creation Date</td>
      <td><%= incident.reported %></td>
    </tr>
    <tr>
      <td><%= link_to "Show this reported incident", incident %></td>
      <td><%= link_to "View in IRONSCALES dashboard", "#{@incident_url}#{incident.incident_id}" %></td>
    </tr>
  </table>
  <br />
<% end %>